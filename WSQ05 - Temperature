#include <iostream>
using namespace std;
int F;
double C;

int main ()
{ F = 57;
  C = (F - 32) / 1.8;
  cout << "The temperature in Fahrenheit is: ";
  cout << F;
  cout << endl;
  cout << "The temperature in Celsius is: ";
  cout << C;
  cout << endl;

  if (C >= 100) {
  cout << "Water does boil at this temperature" << endl;
  }
  if (C < 100) {
  cout << "Water does not boil at this temperature" << endl;
  }

return   0;}
