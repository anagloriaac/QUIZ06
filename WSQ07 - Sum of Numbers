#include <iostream>
using namespace std;

int l;
int u;
int sum;
int a;
int b;

int main ()

{




cout << "We want the sum of integers in a range" << endl;

cout << "Write the lower bound: " << endl;
cin>>l;
cout << "Write the upper bound: " << endl;
cin>>u;

b=0;
sum=0;
a=l+b;

if (l<=u) {
	while (a<u) {
		a= l+b;
		sum=sum+a;
		b=b+1;
	}
	cout << "The sum oof integers in this range is: ";
	cout << sum << endl;
}


return 0;
}
